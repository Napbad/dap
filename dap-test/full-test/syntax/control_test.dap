package main;

fun main() int {

    var int a = 10;
    var int b = 20;

    if a > b {
        write(1, "a > b", 6);
    } else {
        write(1, "a <= b", 6);
    }

    a = 20; 

    if a > b {
        write(1, "a > b", 6);
    } elif a == b {
        write(1, "a == b", 6);
    } else {
        write(1, "a < b", 6);
    }

    a = 30;

    if a > b {
        write(1, "a > b", 6);
    }

    if a < b {
        write(1, "a < b", 6);
    }


    for int i = 0; i < 10; ++i {
        b += i;
    }

    return 0;
}