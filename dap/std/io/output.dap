package dap.std.io;

import dap.std.type.string_d;
import dap.runtime.sys.sysFun;

fun print(imt byte* str) void {
    write(1, str, strlen(str));
    fsync(1);
}

fun println(imt byte* str) void {
    print(str);
    write(1, mkStr((byte)(10)), 2);
    fsync(1);
}

fun newline() void {
    write(1, mkStr((byte)(10)), 2);
    fsync(1);
}